@model Presention.PrintModel.PrintModelOutput
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<form class=" max-w-lg" id="OutputForm">
    
        <div class="flex row -mx-3 mb-6 ">
            <div class="w-full md:w-1/2 px-3 mb-6 md:mb-0 col-6">
            <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="Interwoven">
                همبافت:
            </label>
            <input type="text" disabled asp-for="Interwoven" value="@Model.Interwoven" class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-red-500 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white" >

            </div>
            <div class="w-full md:w-1/2 px-3  col-6">
                <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="Den">
                    دنیر:
                </label>
                <input type="text" asp-for="Den" disabled class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500">
                </input>
            </div>
        </div>
        <div class="flex flex-wrap -mx-3 mb-6 row">
        <div class="w-full md:w-1/2 px-3 mb-6 md:mb-0  col-6">
                <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="Filament">
                    فیلامنت:
                </label>
            <input asp-for="Filament" value="@Model.Filament" disabled class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-red-500 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white" type="text">

            </div>
        <div class="w-full md:w-1/2 px-3  col-6">
                <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="Ply">
                    لا:
                </label>
            <input asp-for="Ply" disabled class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500" type="text">
            </div>
        </div>
        <div class="flex flex-wrap -mx-3 mb-6 row">
        <div class="w-full md:w-1/2 px-3 mb-6 md:mb-0  col-6">
                <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="Mingle">
                    مینگل:
                </label>
            <input asp-for="Mingle" disabled class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-red-500 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white" type="text">

            </div>
        <div class="w-full md:w-1/2 px-3  col-6">
                <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="Personcode">
                    کدپرسنلی:
                </label>
            <input asp-for="Personcode" class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500" type="text">
            </div>
        </div>
        <div class="flex flex-wrap -mx-3 mb-6 row">
        <div class="w-full md:w-1/3 px-3 mb-6 md:mb-0  col-4">
                <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="NumberStr">
                    شماره از:
                </label>
            <input asp-for="NumberStr" class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-red-500 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white" type="number" placeholder="شروع">

            </div>
        <div class="w-full md:w-1/3 px-3  col-4">
                <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="NumberEnd">
                    تا:
                </label>
            <input asp-for="NumberEnd" class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-gray-200 rounded py-3 px-4 leading-tight focus:outline-none focus:bg-white focus:border-gray-500" type="number" placeholder="پایان">
            </div>
        <div class="w-full md:w-1/3 px-3 mb-6 md:mb-0  col-4">
                <label class="block uppercase tracking-wide text-gray-700 text-xs font-bold mb-2" asp-for="MachineCode">
                    دستگاه:
                </label>
            <input asp-for="MachineCode" value="@Model.MachineCode" class="text-center appearance-none block w-full bg-gray-200 text-gray-700 border border-red-500 rounded py-3 px-4 mb-3 leading-tight focus:outline-none focus:bg-white" type="text">
            </div>
        </div>
        <div>
            <input asp-for="Date" value="@Model.Date" class="hidden"/>
        </div>
    <button  type="button" id="SubmitForm" onclick="PrintLabel()" class="btn btn-outline-success">چاپ اطلاعات</button>
    

    

</form>